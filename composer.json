{
    "name": "salzschneider/jira-restclient-php",
    "description": "JIRA API rest client library written in PHP.",
    "keywords": ["JIRA", "API", "PHP", "library", "REST", "client"],
    "license": "MIT",
    "type":"library",
    "authors": [
        {
            "name": "Peter Sovago",
